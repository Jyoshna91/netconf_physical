from ncclient import manager
 
# Define the NETCONF device details
device = {
    'host': 'YOUR_DEVICE_IP',
    'port': 830,
    'username': 'YOUR_USERNAME',
    'password': 'YOUR_PASSWORD',
    'hostkey_verify': False,
    'device_params': {'name': 'default'}
}
 
# NETCONF filter for openconfig-interfaces
openconfig_interfaces_filter = '''
<filter>
http://cisco.com/ns/yang/Cisco-IOS-XE-native">
    <interface>
      <GigabitEthernet>
        <name>1</name>
      </GigabitEthernet>
    </interface>
  </native>
</filter>
'''
 
# NETCONF filter for openconfig-network-instance
openconfig_network_instance_filter = '''
<filter>
<network-instances xmlns="http://openconfig.net/yang/network-instance">
    <network-instance>
      <name>default</name>
    </network-instance>
  </network-instances>
</filter>
'''
 
# Connect to the device and retrieve data for each model
with manager.connect(**device) as m:
    # Load openconfig-interfaces model
    interfaces_reply = m.get(openconfig_interfaces_filter)
    print("OpenConfig Interfaces Model:")
    print(interfaces_reply)
 
    # Load Cisco-IOS-XE-native model
    native_reply = m.get(cisco_ios_xe_native_filter)
    print("Cisco IOS XE Native Model:")
    print(native_reply)
 
    # Load openconfig-network-instance model
    network_instance_reply = m.get(openconfig_network_instance_filter)
    print("OpenConfig Network Instance Model:")
    print(network_instance_reply)
